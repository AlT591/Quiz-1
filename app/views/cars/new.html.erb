<div class="booyah-box col-10 offset-1">
<h1>Enter incoming vehicle Details</h1>
<%= simple_form_for @place do |f| %>
    <%= f.input :make %>
    <%= f.input :model%>
    <%= f.input :year %>
    <%= f.input :color %>
    <br />
    <%= f.submit 'Submit', class: 'btn btn-primary' %>
  <% end %>
</div>